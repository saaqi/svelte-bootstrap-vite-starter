{
  "name": "svelte-bootstrap-vite-starter",
  "private": true,
  "version": "0.0.2",
  "description": "Practice Sveltejs.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "test": "vite preview",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "build-bootstrap": "sass --no-source-map ./src/styles/bs-dynamic.scss ./src/styles/bootstrap.css",
    "push": "git add . && git commit -m \"patch fixes\" && npm version patch && git push"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "sass-embedded": "^1.81.0",
    "svelte": "^5.2.1",
    "vite": "^5.4.11",
    "vite-plugin-minify": "^2.0.1",
    "vite-plugin-webfont-dl": "^3.9.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
